    "gatsby-plugin-catch-links": "^1.0.2",
    "gatsby-plugin-glamor": "^1.6.7",
    "gatsby-plugin-google-analytics": "^1.0.7",
    "gatsby-plugin-react-helmet": "latest",
    "gatsby-plugin-sharp": "^1.6.7",
    "gatsby-plugin-twitter": "^1.0.10",
    "gatsby-plugin-typography": "^1.7.5",
    "gatsby-remark-autolink-headers": "latest",
    "gatsby-remark-copy-linked-files": "^1.5.7",
    "gatsby-remark-prismjs": "latest",
    "gatsby-remark-responsive-iframe": "^1.4.7",
    "gatsby-remark-responsive-image": "^1.0.0-alpha13-alpha.435e0178",
    "gatsby-remark-smartypants": "^1.4.7",
    "gatsby-source-filesystem": "^1.4.11",
    "gatsby-transformer-remark": "^1.7.6",
    "gatsby-transformer-sharp": "^1.6.6",
    "gray-matter": "^3.0.2",
    "instagram-screen-scrape": "^2.0.0",
    "lodash": "^4.15.0",
    "mkdirp": "^0.5.1",
    "progress": "^1.1.8",
    "react-codemirror": "^1.0.0",
    "react-disqus": "^1.0.2",
    "react-disqus-thread": "^0.4.0",
    "react-typography": "^0.15.0",
    "remark": "^7.0.1",
    "slugify": "^1.2.1",
    "typeface-lato": "^0.0.25",
    "typography": "^0.15.8",
    "typography-breakpoint-constants": "^0.14.0",
    "typography-plugin-code": "^0.15.9"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "test": "echo \"Skipping tests\" && exit 0",
    "prebuild": "rm -rf ./public",
    "build": "gatsby build",
    "deploy:prod": "S3PATH=latest ./scripts/deploy.sh",
    "deploy:dev": "S3PATH=dev ./scripts/deploy.sh",
    "deploy": "./scripts/deploy.sh",
    "s3:backup": "aws s3 sync s3://site-headforcode.com/latest s3://site-headforcode.com/revert --delete",
    "s3:upload": "aws s3 sync ./public s3://site-headforcode.com/$S3PATH --acl public-read --sse --delete",
    "s3:revert": "aws s3 sync s3://site-headforcode.com/revert s3://site-headforcode.com/latest --acl public-read --sse --delete"
  }
}
